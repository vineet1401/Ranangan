<!doctype html>
<head>
	<%- include("./DPartials/head.ejs") %>
</head>

<body>

    <%- include("./DPartials/side.ejs") %>



	<!--Content Start-->
	<div class="content-start transition">
		<div class="container-fluid dashboard">
			<div class="content-header">
				<h1>DASHBOARD</h1>
				<p></p>
			</div>

			<div class="row mb-4">

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-id-card  icon-home bg-warning text-light"></i>
								</div>
								<div class="col-8">
									<p>Registration</p>
									<h5>
										<%= registrationcount %>
									</h5>
								</div>
							</div>
						</div>
					</div>

				</div>
				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-inbox icon-home bg-primary text-light"></i>
								</div>
								<div class="col-8">
									<p>Total Matches</p>
									<h5>
										<%= ScheduleCount %>
									</h5>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-clipboard-list icon-home bg-success text-light"></i>
								</div>
								<div class="col-8">
									<p>Completed</p>
									<h5>
										<%= completedCount %>
									</h5>
								</div>
							</div>
						</div>
					</div>
				</div>

				<div class="col-md-6 col-lg-3">
					<div class="card">
						<div class="card-body">
							<div class="row">
								<div class="col-4 d-flex align-items-center">
									<i class="fas fa-chart-bar  icon-home bg-info text-light"></i>
								</div>
								<div class="col-8">
									<p>Pending</p>
									<h5>
										<%= PendingCount %>
									</h5>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="row">
				
				<div class="col-md-6">
					<div class="card">
						<div class="card-body">
							<div class="wrapper">
								<svg>
									<text x="50%" y="50%" dy=".35em" text-anchor="middle">
										GALLERY
									</text>
								</svg>
							</div>

							<img src="images/ezgif.com-effects.gif" style="height: 300px; width: 100%;">
						</div>
					</div>
				</div>

				<div class="col-md-6">
					<div class="card">
						<div class="card-header">
							<h4>Recent Winner</h4>
						</div>
						<div class="card-body pb-4">
							<div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="images/message/4.jpg">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Vineet Sakpal</h5>
									<h6 class="text-muted mb-0">Football</h6>
								</div>
							</div>
							<div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="images/message/1.jpg">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Akshay Raut</h5>
									<h6 class="text-muted mb-0">Kho - Kho</h6>
								</div>
							</div>
							<div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="images/message/5.jpg">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Riya Roy</h5>
									<h6 class="text-muted mb-0">Chess</h6>
								</div>
							</div>
							<div class="recent-message d-flex px-4 py-3">
								<div class="avatar avatar-lg">
									<img src="images/message/1.jpg">
								</div>
								<div class="name ms-4">
									<h5 class="mb-1">Sujeet Pawar</h5>
									<h6 class="text-muted mb-0">Cricket</h6>
								</div>
							</div>
							<div class="px-4">
								<button class='btn btn-block btn-xl btn-primary font-bold mt-3'>See History</button>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>




	<!-- Preloader -->
	<div class="loader">
		<div class="spinner-border text-light" role="status">
			<span class="sr-only">Loading...</span>
		</div>
	</div>

	<!-- Loader -->
	<div class="loader">
		<div class="spinner-border text-light" role="status">
		  <span class="sr-only">...</span>
		</div>
	  </div>
	  <!-- Loader -->
	  <div class="loader-overlay"></div>
	  <!-- General JS Scripts -->
	  <!-- JS Libraies -->
	  <script src="/modules/jquery/jquery.min.js"></script>
	  <script src="/modules/bootstrap-5.1.3/js/bootstrap.bundle.min.js"></script>
	  <!-- Template JS File -->
	  <script src="/js/script.js"></script>
</body>

</html>